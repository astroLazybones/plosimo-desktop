#, kde-format
msgid "Scrolling speed:"
msgstr "Ritināšanas ātrums:"

#: kcm/libinput/touchpad.qml:748
#, kde-format
msgctxt "Slower Scroll"
msgid "Slower"
msgstr "Lēnāks"

#: kcm/libinput/touchpad.qml:755
#, kde-format
msgctxt "Faster Scroll Speed"
msgid "Faster"
msgstr "Ātrāks"

